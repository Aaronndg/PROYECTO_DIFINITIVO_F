{
  "mcpServers": {
    "postgres": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-postgres",
        "postgresql://postgres:[password]@localhost:5432/break_ia"
      ],
      "env": {
        "DATABASE_URL": "postgresql://postgres:[password]@localhost:5432/break_ia"
      }
    },
    "supabase": {
      "command": "node",
      "args": ["mcp-supabase-server.js"],
      "env": {
        "SUPABASE_URL": "your-supabase-url",
        "SUPABASE_SERVICE_KEY": "your-service-role-key",
        "DATABASE_URL": "postgresql://postgres.your-ref:[password]@aws-0-us-west-1.pooler.supabase.com:6543/postgres"
      }
    }
  }
}